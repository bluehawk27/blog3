<h1 id="page_title">Projects I've Built<br><br>
  <% if user_signed_in? %>
    <%= link_to "New Project", new_project_path, class: "button" %>
  <% end %>
</h1>

<div id="posts_wrapper" class="skinny_wrapper">
  <% @project.each do |p| %>
  <div class="post">
    <p class="date"><%= p.created_at.strftime('%A, %B %d') %></p>
    <h2><a href="#"><%= link_to p.title, p %></a></h2>
    <hr>
  </div>
  <% end %>
</div>